{
  "_version": "1.1.0",
  "sap.app": {
    "_version": "1.1.0",
    "id": "mortgage.pawnshop",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProject",
      "version": "1.32.0"
    }
  },
  "sap.ui": {
    "_version": "1.1.0",
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "@res/icon.png",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_belize",
      "sap_bluecrystal"
    ]
  },
  "sap.ui5": {
    "_version": "1.1.0",
    "rootView": {
      "viewName": "mortgage.pawnshop.view.InitView",
      "type": "XML"
    },
    "dependencies": {
      "minUI5Version": "1.30.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {},
        "sap.tnt": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "mortgage.pawnshop.i18n.i18n"
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "mortgage.pawnshop.view",
        "targetAggregation": "pages",
        "clearTarget": false
      },
      "routes": [
        {
          "pattern": "",
          "name": "login",
          "target": "login"
        },
        {
          "pattern": "transaction",
          "name": "transaction",
          "target": "transaction"
        },
        {
          "pattern": "shopConfig",
          "name": "shopConfig",
          "target": "shopConfig"
        },
        {
          "pattern": "regPawnShop",
          "name": "regPawnShop",
          "target": "regPawnShop"
        },
        {
          "pattern": "createTransaction",
          "name": "creTrans",
          "target": "creTrans"
        }
      ],
      "targets": {
        "login": {
          "viewName": "LoginView",
          "viewLevel": 1,
          "controlId": "rootApp",
          "controlAggregation": "pages"
        },
        "mainApp": {
          "viewName": "MainApp",
          "viewLevel": 1,
          "controlId": "rootApp",
          "controlAggregation": "pages"
        },
        "transaction": {
          "parent": "mainApp",
          "viewName": "Transaction",
          "viewLevel": 2,
          "controlId": "mainApp",
          "controlAggregation": "pages"
        },
        "shopConfig": {
          "parent": "mainApp",
          "viewName": "ShopConfig",
          "viewLevel": 2,
          "controlId": "mainApp",
          "controlAggregation": "pages"
        },
        "regPawnShop": {
          "viewName": "RegisterPawnShop",
          "viewLevel": 2,
          "controlId": "rootApp",
          "controlAggregation": "pages"
        },
        "creTrans": {
          "viewName": "CreateTransaction",
          "viewLevel": 2,
          "controlId": "rootApp",
          "controlAggregation": "pages"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    }
  }
}